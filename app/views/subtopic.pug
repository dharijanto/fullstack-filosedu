html
  head
    title Aljabar Linear
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

  body
    nav.navbar.navbar-default
      .container-fluid
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#bs-example-navbar-collapse-1", aria-expanded="false")
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href="/") Filosedu
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav
            li.active
              a(href="/") Homepage
            li
              a(href="#") Course
        // /.navbar-collapse
      // /.container-fluid

    div.container
      each val, index in data
        h1.text-center #{val.dataValues.subtopic}
        h4.text-center #{val.dataValues.description}
        hr
        - var detail = JSON.parse(val.dataValues.data)
          | !{detail.detail_course}

          iframe(width="100%", height="315", src=`//youtube.com/embed/${detail.link_youtube}`, frameborder="0", allowfullscreen="")

      div.text-center(style="margin-top:50px;margin-bottom:50px;")
        a.btn.btn-primary(href="/question") START COURSE

    script(src="https://code.jquery.com/jquery-3.2.1.js")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")
